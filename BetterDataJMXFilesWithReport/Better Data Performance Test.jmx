              <stringProp name="script">import java.util.Base64;

// Fetch the User Defined Variables
String originalFilename = vars.get(&quot;original_filename&quot;);
String originalFiletype = vars.get(&quot;original_filetype&quot;);
String originalDescription = vars.get(&quot;original_description&quot;);
String originalSource = vars.get(&quot;original_source&quot;);
String originalDataName = vars.get(&quot;original_data_name&quot;);

// Encode each variable in Base64
String encodedFilename = Base64.getEncoder().encodeToString(originalFilename.getBytes(&quot;UTF-8&quot;));
String encodedFiletype = Base64.getEncoder().encodeToString(originalFiletype.getBytes(&quot;UTF-8&quot;));
String encodedDescription = Base64.getEncoder().encodeToString(originalDescription.getBytes(&quot;UTF-8&quot;));
String encodedSource = Base64.getEncoder().encodeToString(originalSource.getBytes(&quot;UTF-8&quot;));
String encodedDataName = Base64.getEncoder().encodeToString(originalDataName.getBytes(&quot;UTF-8&quot;));

// Construct the Upload-Metadata header value
String uploadMetadata = &quot;filename &quot; + encodedFilename + &quot;,filetype &quot; + encodedFiletype +
                        &quot;,description &quot; + encodedDescription + &quot;,source &quot; + encodedSource +
                        &quot;,data_name &quot; + encodedDataName;

// Store the Upload-Metadata in a JMeter variable
vars.put(&quot;uploadMetadata&quot;, uploadMetadata);

// Optionally log the metadata for debugging
log.info(&quot;Upload-Metadata: &quot; + uploadMetadata);
</stringProp>
